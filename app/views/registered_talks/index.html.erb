<p id="notice"><%= notice %></p>

<h1>Registered Talks</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Talk</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registered_talks.each do |registered_talk| %>
      <tr>
        <td><%= registered_talk.profile_id %></td>
        <td><%= registered_talk.talk_id %></td>
        <td><%= link_to 'Show', registered_talk %></td>
        <td><%= link_to 'Edit', edit_registered_talk_path(registered_talk) %></td>
        <td><%= link_to 'Destroy', registered_talk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registered Talk', new_registered_talk_path %>
